{
    "manifest_version": 2,

    "name": "right key",
    "description": "This extension demonstrates a 'browser action' with kittens.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "main.html"
    },

    "permissions": [
        "contextMenus", "tabs", "storage"
    ],

    "background":{
        "scripts": ["menu.js", "translate.js"]
    }
}
